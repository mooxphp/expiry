{
    "name": "moox/expiry",
    "description": "Monitor the expiration of records",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "5.0.11"
    },
    "require-dev": {
        "moox/devtools": "5.0.11"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Expiry\\": "src",
            "Moox\\Expiry\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Expiry\\ExpiryServiceProvider"
            ]
        },
        "moox": {
            "stability": "stable"
        }
    },
    "prefer-stable": true
}